$main::objbase->[745] = sub { my $i = npc->new('LIMIT', 13, 'NAME', 'Asp', 'VOL', 9, 'FPAHD', 'hit', 'FPSHD', 'hit', 'TPSHD', 'hits', 'TPAHD', 'hit', 'AC', 200, 'DWEAPNAME', 'fangs', 'MASS', 225, 'WC', 100, 'DESC', 'This large snake slowly slithers across the ground. At first glance it appears rather harmless, but upon closer inspection you can see thin red stripes on the back of its head, marking it as poisonous.'); $i->stats_allto(100, 130); return($i); };
